{
  "compilerOptions": {
    "extendedDiagnostics": false,
    "strict": false,
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "noImplicitAny": false,
    "noEmitOnError": false
  },
  "include": ["src"],
  "exclude": []
}
